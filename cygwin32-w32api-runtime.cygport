CROSS_HOST="i686-pc-cygwin"
inherit cross

NAME="cygwin32-w32api-runtime"
VERSION=3.1.0
RELEASE=1
CATEGORY="Devel"
SUMMARY="Win32 API import libs for Cygwin 32bit toolchain"
DESCRIPTION=
