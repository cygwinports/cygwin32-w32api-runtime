SRC_DIR="mingw-w64-v${VERSION}/mingw-w64-crt"

cygwin32_w32api_runtime_REQUIRES="cygwin32-w32api-headers"

CYGCONF_ARGS="--enable-w32api --enable-lib32 --disable-lib64"
